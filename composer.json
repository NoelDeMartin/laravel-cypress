{
    "name": "noeldemartin/laravel-cypress",
    "description": "Laravel package used for running tests with Cypress.",
    "keywords": ["laravel", "cypress", "testing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Noel De Martin",
            "email": "noeldemartin@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": ">=5.5.0"
    },
    "autoload": {
        "psr-4" : {
            "NoelDeMartin\\LaravelCypress\\" : "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NoelDeMartin\\LaravelCypress\\CypressServiceProvider"
            ]
        }
    }
}
